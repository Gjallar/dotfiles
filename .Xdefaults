! urxvt*font: xft:Bitstream Vera Sans Mono:size=10
urxvt*font: xft:Inconsolata-XL:size=10
urxvt*scrollBar: false
urxvt*internalborder: 6
urxvt.urgentOnBell: true
urxvt*foreground: #f2f2f2
urxvt*background: #101010

! Zenburn Colors 
UURxvt.foreground: #dcdccc

!URxvt.depth: 32
URxvt*background: #3f3f3f

URxvt.saveLines:32767

URxvt.perl-lib: ~/dotfiles/urxvt-perls/
URxvt.perl-ext-common: default,matcher,keyboard-select,clipboard,tabbed
URxvt.keysym.M-Escape: perl:keyboard-select:search
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.urlLauncher: uzbl-browser
URxvt.matcher.button: 2
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

