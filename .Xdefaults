! urxvt*font: xft:Bitstream Vera Sans Mono:size=10
urxvt*font: xft:Inconsolata-XL:size=10
urxvt*scrollBar: false
urxvt*internalborder: 6
urxvt.urgentOnBell: true

URxvt.saveLines:32767

URxvt.perl-lib: ~/dotfiles/urxvt-perls/
URxvt.perl-ext-common: default,matcher,keyboard-select,clipboard,tabbed
URxvt.keysym.M-Escape: perl:keyboard-select:search
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.urlLauncher: uzbl-browser
URxvt.matcher.button: 2
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

*foreground: #DDCCBB

! Brown BG
!*background: #30251D

! Grey BG
*background: #151515


!black
*color0:     #393939
*color8:     #AF875F
!red
*color1:     #BF1E2D
*color9:     #E7212A
!green
*color2:     #9DBA3A
*color10:    #A9C938
!yellow
*color3:     #F5B915
*color11:    #F7D325
!blue
*color4:     #1CA1DB
*color12:    #00AFDA
!magenta
*color5:     #652F90
*color13:    #894E9F
!cyan
*color6:     #EA7D24
*color14:    #F79321
!white
*color7:     #E7E8E9
*color15:    #F3F3F4
